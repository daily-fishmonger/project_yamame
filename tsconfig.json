{
  "compilerOptions": {
    "target":"esnext",
    "module":"esnext",
    "strict":true,
    "importHelpers":true,
    "moduleResolution":"node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxt/content",
    ],
    "resolveJsonModule": true,
  },
  "exclude": [
    "node_modules"
  ]
}
